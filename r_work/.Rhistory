pull(proj_id) %>%
paste(collapse = ",")
q <- paste0(
"SELECT id, bytes, filename, mimetype, folder_id, project_id
FROM document
WHERE project_id IN (", proj_ids ,")
AND deleted IS NULL
AND folder_id IS NOT NULL"
)
remove(proj_ids)
all_docs <- dbGetQuery(con, q)
